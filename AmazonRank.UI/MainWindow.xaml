<Window x:Class="AmazonRank.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AmazonRank.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <RichTextBox HorizontalAlignment="Left" Height="182" Margin="0,237,0,0" VerticalAlignment="Top" Width="792" 
                     ScrollViewer.CanContentScroll="True" Name="Output" 
                     ScrollViewer.VerticalScrollBarVisibility="Auto">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
        <Button Content="Button" HorizontalAlignment="Left" Margin="232,14,0,0" VerticalAlignment="Top" Width="75" Name="LoadBtn" Click="LoadBtn_Click"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="18,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="196" Name="Link" />
        <Button Content="Button" HorizontalAlignment="Left" Margin="29,68,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
</Window>
